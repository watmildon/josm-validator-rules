meta
{
    title: "Feet to Meter converter";
    version: "0,1_2024-04-26";
    description: "Converts HEIGHT in ft to height in meters";
    author: "watmildon";
    link: "https://github.com/watmildon/josm-validator-rules/blob/main/rules/FeetToMeterConverter.validator.mapcss";
    baselanguage: "en";
    min-josm-version: 14481;
}

*["HEIGHT"] {
    throwWarning: tr("Convert to meters");
    fixAdd: concat("height=", round(tag("HEIGHT") * 0.3048 * 100) / 100);
    fixRemove: "HEIGHT";
}
